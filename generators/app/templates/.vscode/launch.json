{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "node",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/src/index.js",
      "env": {
        "USE_DOTENV": "true",
        "NODE_ENV": "development"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "windows": {
        "useWSL": true,
        "env": {
          "USE_DOTENV": "true",
          "NODE_ENV": "development",
          "WSLENV": "NODE_ENV:USE_DOTENV"
        }
      }
    }
  ]
}
