{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "node",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/dev.js",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PORT": "3000",
        "NODE_ENV": "development"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
